  <h1>My Attending Events</h1>
  <h3>Past Events</h3>
  <ul>
    <% @past_events.each do |event| %>  
      <li>
        <%= link_to "#{event.date} - #{event.location}", event_path(event) %>
      </li>    
    <% end %>
  </ul>    
  <h3>Upcoming Events</h3>
  <ul>
    <% @upcoming_events.each do |event| %>    
      <li>
        <%= link_to "#{event.date} - #{event.location}", event_path(event) %>
      </li>
    <% end %>
  </ul>